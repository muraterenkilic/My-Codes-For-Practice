class Solution {
    public int[] twoSum(int[] nums, int target) {
        int forward = 1;
        for (int i = 0; i < nums.length; i++)
        {
            for (int j = forward; j < nums.length; j++)
            {
                if (nums[i] + nums[j] == target)
                {
                    return new int[] {i, j};
                }
            }
            forward++;
        }
        return new int[] {};
    }
}
