class Solution {
    public int maximumDifference(int[] nums) {
        int biggestDifference = -1;
        int smallest = 1000000000;
        for (int i = 1; i < nums.length; i++)
        {
            if (nums[i - 1] < smallest)
            {
                smallest = nums[i - 1];
            }
            if (nums[i] - smallest > biggestDifference && nums[i] - smallest >= 1 )
            {
                biggestDifference = nums[i] - smallest;
            }
        }
        return biggestDifference;
    }
}
